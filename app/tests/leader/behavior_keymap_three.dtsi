#include <dt-bindings/zmk/keys.h>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/kscan_mock.h>

&leader {
	timeout-ms = <200>;
	timeout-on-activation;
};

/ {
	leader-sequences {
		compatible = "zmk,leader-sequences";

		leader_seq_one {
			key-positions = <0 1 2>;
			bindings = <&kp A>;
		};

	};

	keymap {
		compatible = "zmk,keymap";
		label ="Default keymap";

		default_layer {
			bindings = <
				&kp N1 &kp N2
				&kp N3 &leader
			>;
		};
	};
};
